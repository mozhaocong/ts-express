{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"outDir": "./dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": "src",
		"typeRoots": ["express"]
	},
	"exclude": ["node_modules"],
	"include": ["./src/**/*.ts"]
}
